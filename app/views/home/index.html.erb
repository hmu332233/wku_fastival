<script>
  
//좋아요
 $(".like-btn").click(function(){
        var id = this.value;
        var ho = parseInt($("#"+id+"-hidden").val());
        var no = $("#"+id+"-no").val();
        var he = ho+1;
            if(no==0){
               $.ajax({
                    url:'/home/ho',
                    method:"get",
                    data:{"jujum_id":id},
                    success:function(data){
                        $("#"+id+"-count").text(he);
                    },
                    error:function(){
                     }
                })
            }
        });
</script>

    