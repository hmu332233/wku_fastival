<!--주점섹션임-->
    <section class="success3" id="jujum">
    <div class="container">
      <div class="row">
          <div class="col-lg-12 text-center">
              <h2>주점정보</h2>
              <hr class="star-light">
          </div>
      </div>
      <div class="row">
        <!--지도 섹션임-->
        <div id="map"></div>
        <script type="text/javascript" src="//apis.daum.net/maps/maps3.js?apikey=da11ef6f3596510174ab5e0c45bb6759"></script>
        <!-- 주점정보 -->
        
        <div class="acc">
          
          <% @jujums.each do |j| %>
            <button class="accordion" id="acc-panel<%=j.id%>-btn"><%= j.name%> <span>&#9662;</span></button>
            <div class="panel" id="acc-panel<%= j.id%>" >
              <button class="like-btn"value="<%= j.id%>"><img src="/img/like.png"/></button>
              <input type="number" id="<%= j.id%>-hidden"value="<%=@merong_count[j.id]%>" style="display:none;"/>
              <input type="hidden" value="<%= @merong_no[j.id]%>" id="<%=j.id%>-no"/>
              <span id="<%= j.id%>-count" value="<%= j.id%>"><%=@merong_count[j.id]%></span>
              <img src="/img/portfolio/cabin.png"/>
              <p>컴공은짱이얌 ㅎㅅㅎㅅㅎㅅㅎ~!!!</p>
            </div>
          <% end %>
          
        </div>
    </div>
    </section>



